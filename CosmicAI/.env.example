# .env.example

# Ethereum Network Configuration
NODE_URL=http://localhost:8545 # URL of the Ethereum node (e.g., Ganache, Infura)
NETWORK=development # Network name (e.g., development, mainnet, ropsten)

# Contract Deployment Configuration
CONTRACT_ADDRESS=0xYourContractAddress # Address of the deployed contract
PRIVATE_KEY=0xYourPrivateKey # Private key for the account deploying the contract
ACCOUNT_ADDRESS=0xYourAccountAddress # Account address for transactions

# Database Configuration (if applicable)
DB_HOST=localhost # Database host
DB_PORT=5432 # Database port
DB_USER=your_db_user # Database username
DB_PASSWORD=your_db_password # Database password
DB_NAME=your_db_name # Database name

# API Keys (if applicable)
API_KEY=your_api_key # API key for external services
ANOTHER_API_KEY=another_api_key # Another API key

# Miscellaneous Configuration
LOG_LEVEL=info # Logging level (e.g., debug, info, warn, error)
